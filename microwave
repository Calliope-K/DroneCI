#!/bin/bash
# your code goes here
neofetch
git clone https://github.com/xanmod/linux -b 5.15
cd linux
git fetch https://github.com/vcyzteen/rodjogdam.git 5.15-musl
git cherry-pick bef01beaa7829ff5620c6bf00c86453d5487d4ce
cp CONFIGS/xanmod/gcc/config .config
make oldconfig
make
sudo make modules_install 
sudo make install
ls /boot
ls kernel
