#!/bin/bash
# your code goes here
neofetch
git clone https://github.com/vcyzteen/rodjogdam.git -b 5.15
cd rodjogdam
wget -O .config https://raw.githubusercontent.com/void-linux/void-packages/master/srcpkgs/linux5.15/files/x86_64-dotconfig
sed -i -e "s|^\(CONFIG_LOCALVERSION=\).*|\1\"_rodjogdam\"|" .config
export KBUILD_BUILD_USER=rodjogdam
export KBUILD_BUILD_HOST=cloud.drone.io
make -j$(nproc --all) ARCH=x86_64 prepare
make -j$(nproc --all) ARCH=x86_64 INSTALL_MOD_PATH=1 modules_install
git status
# git clone https://github.com/vcyzteen/linux.git -b 4.4-bfq
# cd linux
# wget https://raw.githubusercontent.com/vcyzteen/myscripts/min/kernel.sh
# bash kernel.sh
