#!/bin/sh
# your code goes here
git clone --depth=1 https://github.com/GZR-Kernels/Optimus_Drunk_Alioth.git -b 11.0
cd Optimus_Drunk_Alioth
export KBUILD_BUILD_USER=vcyzteen
export KBUILD_BUILD_HOST=voidlinux
make -j$(nproc) O=out ARCH=arm64 vendor/alioth_defconfig
make -j$(nproc) ARCH=arm64 O=out \
                CROSS_COMPILE=aarch64-linux-gnu- \
                CROSS_COMPILE_ARM32=arm-linux-gnueabi- \
                CC=clang \
                AR=llvm-ar \
                OBJDUMP=llvm-objdump \
                STRIP=llvm-strip \
                NM=llvm-nm \
                OBJCOPY=llvm-objcopy \
                LD=ld.lld
# wget https://raw.githubusercontent.com/vcyzteen/myscripts/clang/kernel.sh
# bash kernel.sh
ls out/arch/arm64/boot
