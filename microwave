cp msm-4.4/arch/arm64/configs/sdm660_defconfig linux/arch/arm64/configs/sdm660_defconfig
cd linux
# git reset --hard 6807d861c1253a6c82f8bc3bf128ea97b738ba68
# git cherry-pick be42cc151d4dbeec0dae579d0fd69d79835b1393 be42cc151d4dbeec0dae579d0fd69d79835b1393 b9c691895d3492b2c698be5367487a645f239ac2
